{
  "name": "superlimiter",
  "description": "frequency limiter for node.js",
  "version": "1.0.1",
  "author": "Tree Xie <vicansocanbico@gmail.com>",
  "keywords": [
    "frequency",
    "limit",
    "limiter",
    "koa limit",
    "express limit"
  ],
  "scripts": {
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "node ./node_modules/.bin/eslint ./lib/*.js && node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./test/*.js --"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicanso/superlimiter.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.15.2",
    "ioredis": "^2.5.0",
    "istanbul": "^0.4.5",
    "koa": "^2.2.0",
    "mocha": "^3.1.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "lodash": "^4.17.4"
  }
